{
  "name": "vscode-page",
  "author": "dteam",
  "publisher": "dteam",
  "version": "0.0.2",
  "description": "A light-weight page micro framework for vscode webview.",
  "license": "Apache-2.0",
  "tags": [
    "functions",
    "classes",
    "typescript",
    "tools",
    "helpers",
    "vscode"
  ],
  "keywords": [
    "functions",
    "classes",
