    "extensions",
    "modules"
  ],
  "engines": {
    "vscode": "^1.42.0"
  },
  "main": "lib/pages.js",
  "typings": "lib/pages.d.ts",
  "scripts": {
    "prepare": "rm -rf lib && tsc && tsc -d",
    "prepublishOnly": "npm run compile && npm run lint",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "mocha out/test/**/*.test.js",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "posttest": "npm run check"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/vscode": "^1.42.0",
    "@types/node": "^12.11.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "mocha": "^10.2.0",
    "typescript": "^3.7.5",
    "handlebars": "^4.7.3",
    "gts": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTeam-Top/vscode-page.git"
  },
  "bugs": {
    "url": "https://github.com/DTeam-Top/vscode-page/issues"
